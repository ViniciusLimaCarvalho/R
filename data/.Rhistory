calcula_probabilidade <- function(n){
resultados<-c()
i <- 0
for(i in 1:10000){
aniversarios <- sample(x = 1:365, size = n, replace = TRUE)
resultados[i] <- any(duplicated(aniversarios))
}
print(mean(resultados))
}
